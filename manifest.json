{
  "manifest_version": 2,

  "name": "ProofMe-Canva",
  "description": "A quick way to import your canva project into a proof.",
  "version": "0.1",


  "content_scripts": [
      {
          "matches": ["https://www.canva.com/*"],
          "js": ["jquery-3.0.0.min.js", "send-to-proofme.js"]
      }

  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "ProofMe"
  },

  "permissions": [
    "https://www.canva.com/*"
  ]
}
